{
  "entrypoint": "app/index.html",
  "shell": "app/src/my-app.html",
  "fragments": [
    "app/src/my-view1.html",
    "app/src/my-view2.html",
    "app/src/my-view3.html",
    "app/src/my-view404.html"
  ],
  "sources": [
    "app/src/**/*",
    "app/images/**/*"
  ],
  "extraDependencies": [
    "app/manifest.json",
    "app/bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
